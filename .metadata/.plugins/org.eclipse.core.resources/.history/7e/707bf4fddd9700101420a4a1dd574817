 * Descripción: Ejemplos de tipos de variable
 * Autor: Cristian González Mateo
 * Fecha: 22/09/25
 */
package tiposVariables;

public class EjemplosVariables {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int numAlumnos; // Declaramos la variable de numAlumnos
		numAlumnos = 0;
		System.out.println("valor de la variable:" + numAlumnos);
		
		numAlumnos = 12;
		System.out.println("valor de la variable:" + numAlumnos);
		
		double notaAlumnos = 1.75; //Declaramos la variable de notaAlumnos
		System.out.println("valor de la variable:" + notaAlumnos);
		
		char letraDni;
		letraDni = 'a';
		System.out.println("valor de la variable:" + letraDni);
		
		String nombreAlumnos = "Cristian";
		System.out.println("valor de la variable:" + nombreAlumnos);
		
		final int notaMax = 10;
		System.out.println("valor de la variable" + notaMax);
	
		
	}
       
}
