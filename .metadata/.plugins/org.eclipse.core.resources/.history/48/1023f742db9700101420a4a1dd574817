package tiposVariables;

public class EjemplosVariables {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int numAlumnos; // Declaramos la variable de numAlumnos
		numAlumnos = 0;
		double notaAlumnos; // Variable notaAlumnos
		notaAlumnos = 7.5;
		System.out.print("valor de la variable: " + numAlumnos);

	}

}
